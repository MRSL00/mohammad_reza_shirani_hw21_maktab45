    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= (()=> {
            if (page === "profile") {return "Profile"}
            else if(page === "Register") {return "Register"}
            else if(page === "Edit profile") {return "Edit profile"}
            else if(page === "allarticles") {return "All Articles"}
            else if(page === "myarticles") {return "My Articles"}
            else if(page === "createarticle") {return "Create Article"}
