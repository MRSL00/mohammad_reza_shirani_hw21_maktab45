    <link rel="stylesheet" href=<%=(()=> {
    if (page === "profile") {return "/stylesheets/profile.css"}
    else if (page === "Edit profile") {return "/stylesheets/profile.css"}
    else if(page === "Register") {return "/stylesheets/form.css"}
    else if(page === "Login") {return "/stylesheets/form.css"}
    else if(page === "allarticles") {return "/stylesheets/allarticles.css"}
    else if(page === "myarticles") {return "/stylesheets/myarticles.css"}
    else if(page === "showarticle") {return "/stylesheets/showarticle.css"}
